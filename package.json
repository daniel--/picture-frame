{
  "name": ".",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src/server -w srs/shared -w .env -x tsx src/server/main.ts",
    "start": "NODE_ENV=production tsx src/server/main.ts",
    "build": "vite build",
    "db:push": "drizzle-kit push",
    "create-user": "npx tsx src/scripts/create-user.ts",
    "docker:build": "GIT_HASH=$(git rev-parse HEAD 2>/dev/null || echo 'unknown') docker-compose build",
    "docker:up": "GIT_HASH=$(git rev-parse HEAD 2>/dev/null || echo 'unknown') docker-compose up -d",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,html,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,html,md}\"",
    "prepare": "node -e \"const fs=require('fs'),path=require('path');const huskyPath=path.join(process.cwd(),'node_modules','.bin','husky');if(fs.existsSync(huskyPath)){require('child_process').execSync('husky',{stdio:'inherit'});}\""
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,html,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@libsql/client": "^0.15.15",
    "@react-spring/web": "^10.0.3",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/ws": "^8.18.1",
    "@use-gesture/react": "^10.3.1",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.44.7",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.9.6",
    "react-use-websocket": "^4.13.0",
    "sharp": "^0.34.5",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3",
    "usehooks-ts": "^3.1.1",
    "vite-express": "*",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "vite": "^6.3.3",
    "vite-plugin-pwa": "^1.2.0",
    "workbox-core": "^7.4.0",
    "workbox-precaching": "^7.4.0",
    "workbox-routing": "^7.4.0",
    "workbox-window": "^7.4.0"
  },
  "overrides": {
    "esbuild": ">=0.25.0"
  }
}
